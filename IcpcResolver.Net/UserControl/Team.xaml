<UserControl x:Class="IcpcResolver.Net.UserControl.Team"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local ="clr-namespace:IcpcResolver.Net.UserControl"
             mc:Ignorable="d"
             DataContext="{Binding RelativeSource={RelativeSource Self}}"
             d:DesignHeight="Auto" d:DesignWidth="*">
    <StackPanel Orientation="Horizontal">
        <Label Content="{Binding TeamRank}"/>
        <Label Content="{Binding TeamName}"/>
    </StackPanel>
</UserControl>
